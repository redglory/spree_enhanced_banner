<%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @image } %>

<% content_for :page_title do %>
  <%= Spree.t(new_banner_for) %> <%= @taxon.name %>
<% end %>

<%= form_for [:admin, @image], :url => admin_taxon_enhanced_banners_path(:taxon_id => @taxon.id), :html => { :multipart => true } do |f| %>
  <fieldset data-hook="new_banner">
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>